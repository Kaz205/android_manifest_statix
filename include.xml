           revision="tm-qpr3"
           fetch="https://github.com/StatiXOS/" />

  <!-- STX-Staging -->
  <remote  name="staging"
	   revision="udc"
	   fetch="https://github.com/stx-staging/" />

  <remote  name="kazuki"
	   revision="udc"
	   fetch="https://github.com/Kaz205/" />

  <remote  name="r3"
	   revision="refs/tags/android-14.0.0_r3"
